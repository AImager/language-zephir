{
    "name": "language-zephir",
    "displayName": "language-zephir",
    "description": "language support for zephir",
    "version": "0.1.0",
    "publisher": "AImager",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Languages"
    ],
    "homepage": "https://github.com/AImager/language-zephir",
    "bugs": {
        "url": "https://github.com/AImager/language-zephir/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AImager/language-zephir.git"
    },
    "contributes": {
        "languages": [{
            "id": "zephir",
            "aliases": ["Zephir", "zephir"],
            "extensions": [".zep"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "zephir",
            "scopeName": "source.php.zephir",
            "path": "./syntaxes/Zephir.tmLanguage"
        }]
    }
}